<div class="container">
    <div class="row">
        <div class="col-sm">
          <p class="h5">Tossups <%- totalT %> / 160</p>
          <hr/>
          <% data.forEach(function(d){ %>
          <p class="h6"><%= d.cat %>:  <%= d.val %> / <%= d.tot %></p>
          <div class="progress" style="height: 30px;">
            <div class="progress-bar bg-success" role="progressbar" style=<%- "width:"  +((d.val/d.tot) * 100) +"%" %> aria-valuenow="<%- (d.val/d.tot)*100 %>" aria-valuemin="0" aria-valuemax="100">
              <%- Math.floor((d.val/d.tot)*100)%>%
            </div>
          </div>
          <br>
          <% }); %>
        </div>
        <div class="col-sm">
          <p class="h5">Bonuses <%- totalB %> / 160</p>
          <hr/>
          <% bonus.forEach(function(d){ %>
          <p class="h6"><%= d.cat %>:  <%= d.val %> / <%= d.tot %></p>
          <div class="progress" style="height: 30px;">
            <div class="progress-bar bg-success" role="progressbar" style=<%- "width:"  +((d.val/d.tot) * 100) +"%" %> aria-valuenow="<%- (d.val/d.tot)*100 %>" aria-valuemin="0" aria-valuemax="100">
              <%- Math.floor((d.val/d.tot)*100)%>%
            </div>
          </div>
          <br>
          <% }); %>
        </div>
      </div>
</div>